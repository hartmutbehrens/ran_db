<tables>
	<table name="3GAlarms">
		<field name="EVENTTIME" type="datetime" source="import">
		</field>
		<field name="CLEARTIME" type="datetime" source="import">
		</field>
		<field name="NEUSERLABEL" type="varchar(255)" source="import">
		</field>
		<field name="OMC_ID" type="varchar(50)" source="import">
		</field>
		<field name="SUBCOMPONENTID" type="varchar(255)" source="import">
		</field>
		<field name="EVENTTYPE" type="varchar(50)" source="import">
		</field>
		<field name="PERCEIVEDSEVERITY" type="varchar(50)" source="import">
		</field>
		<field name="ACTIVESTATUS" type="varchar(50)" source="import">
		</field>
		<field name="PROBABLECAUSE" type="varchar(255)" source="import">
		</field>
		<field name="SPECIFICPROBLEM" type="varchar(255)" source="import">
