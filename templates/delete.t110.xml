		 </step>
		 <step name="2" from="T110_SECTOR_D" identifier="BSC_ID,BTS_ID,SECTOR,OMC_ID,BSC_NAME,LAC,CI" using="SDATE" keep="360">
		 </step>
	</delete>
</deletions>
